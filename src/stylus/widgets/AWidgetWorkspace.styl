// Contains everything interesting, including workspace and sidebars
.aworkspace
  background-color $col_workspace_bg
  width 100%

  #aw-canvas-container
    display block
    margin 0 auto
    position relative

  #awcc-outline
    border 1px solid #0099cc
    background-color transparent
    position absolute
    pointer-events none

  #awcc-outline-text
    color #0099cc
    position absolute
    font-size 12px